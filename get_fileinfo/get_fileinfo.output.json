{
  "$schema": "https://json-schema.org/draft-07/schema",
  "properties": {
    "CompanyName": {
      "type": "string"
    },
    "CreationTime": {
      "type": "integer"
    },
    "FileDescription": {
      "type": "string"
    },
    "FileName": {
      "type": "string",
      "format": "localFilePath"
    },
    "FileVersion": {
      "type": "string"
    },
    "LastAccessTime": {
      "type": "integer"
    },
    "LastWriteTime": {
      "type": "integer"
    },
    "Length": {
      "type": "integer"
    },
    "Mode": {
      "type": "string"
    },
    "OriginalFilename": {
      "type": "string"
    },
    "ProductName": {
      "type": "string"
    },
    "Sha256": {
      "type": "string",
      "format": "sha256"
    }
  },
  "required": [
    "Length",
    "CreationTime",
    "LastWriteTime",
    "LastAccessTime",
    "FileName",
    "Sha256"
  ],
  "type": "object"
}
